{
  "name": "krgupta/active",
  "description": "The helper class for Laravel 6 applications to get active class base on current route",
  "keywords": [
    "laravel",
    "active",
    "routing"
  ],
  "license": "MIT",
  "homepage": "",
  "authors": [
    {
      "name": "Irfan Momin",
      "email": "irfan.momin990@gmail.com",
      "homepage": ""
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "orchestra/testbench": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Krgupta\\Active\\": "src/",
      "Krgupta\\ActiveTest\\": "tests/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Krgupta\\Active\\Providers\\ActiveServiceProvider"
      ],
      "aliases": {
        "Active": "Krgupta\\Active\\Facades\\Active"
      }
    }
  }
}
